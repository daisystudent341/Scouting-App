<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:columnCount="6"
    android:rowCount="9">


    <TextView
        android:id="@+id/text_HighGoalT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:layout_column="1"
        android:text="High Goal"
        android:textSize="35dp"
        android:layout_gravity="center_horizontal"

        />


    <Button
        android:id="@+id/button_AutonomousT"
        android:layout_width="wrap_content"
        android:layout_height="113dp"
        android:layout_marginStart="30dp"
        android:layout_row="7"
        android:layout_column="1"
        android:layout_gravity="top"
        android:text="Back"
        android:textSize="25dp" />

    <Button
        android:id="@+id/button_HighMinusT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="0"
        android:layout_gravity="end"
        android:text="-"
        android:textSize="90dp" />

    <TextView
        android:id="@+id/text_HighValueT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="1"
        android:layout_gravity="center"
        android:text="0"
        android:textSize="90dp" />

    <Button
        android:id="@+id/button_HighPlusT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="2"


        android:text="+"
        android:textSize="90dp" />

    <TextView
        android:id="@+id/text_lowGoalT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:layout_column="4"
        android:text="Low Goal"
        android:layout_gravity="center_horizontal"

        android:textSize="35dp" />

    <Button
        android:id="@+id/button_lowMinusT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="3"
        android:layout_gravity="end"
        android:text="-"
        android:textSize="90dp" />

    <TextView
        android:id="@+id/text_LowValueT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="4"
        android:text="0"
        android:layout_gravity="center"

        android:textSize="90dp" />


    <Button
        android:id="@+id/button_LowPlusT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="5"

        android:text="+"
        android:textSize="90dp" />

    <TextView
        android:id="@+id/text_High_MissedT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="1"
        android:text="High Missed"
        android:textSize="35dp" />

    <Button
        android:id="@+id/button_HighMissedPlusT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="2"
        android:text="+"
        android:textSize="90dp" />

    <TextView
        android:id="@+id/text_HighMissedValueT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="1"
        android:layout_gravity="center"
        android:text="0"
        android:textSize="90dp" />

    <Button
        android:id="@+id/button_HighMissedMinusT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="0"
        android:layout_gravity="end"
        android:text="-"
        android:textSize="90dp" />

    <Button
        android:id="@+id/button_LowMissedMinusT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="3"
        android:layout_gravity="end"
        android:text="-"
        android:textSize="90dp" />

    <TextView
        android:id="@+id/text_Low_MissedT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="4"
        android:text="Low Missed"
        android:layout_gravity="center"

        android:textSize="35dp" />

    <TextView
        android:id="@+id/text_Low_Missed_ValueT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="4"
        android:layout_gravity="center"

        android:text="0"
        android:textSize="90dp" />

    <Button
        android:id="@+id/button_LowMissedPlusT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="5"
        android:text="+"
        android:textSize="90dp" />



    <TextView
        android:id="@+id/text_Teleop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="5"
        android:layout_column="0"
        android:paddingStart="50dp"
        android:text="TELEOP"

        android:textSize="35dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/text_userInfoT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="6"
        android:layout_column="0"
        android:paddingStart="50dp"
        android:layout_gravity="top"
        android:text="Info"
        android:textSize="35dp"
        android:textStyle="italic" />


    <Button
        android:id="@+id/button_quitT"
        android:layout_width="182dp"
        android:layout_height="113dp"
        android:layout_row="7"
        android:layout_column="0"
        android:layout_gravity="top"
        android:text="Quit"
        android:textSize="35dp" />

    <Button
        android:id="@+id/button_SubmitT"
        android:layout_width="182dp"
        android:layout_height="113dp"
        android:layout_row="7"
        android:layout_column="5"
        android:layout_gravity="center"

        android:text="Submit"
        android:textSize="35dp" />

    <CheckBox
        android:id="@+id/checkBox_Mid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="4"
        android:layout_column="3"
        android:scaleX="2"
        android:scaleY="2"
        android:text="Mid"
        android:layout_gravity="center"

        />

    <CheckBox
        android:id="@+id/checkBox_NoClimb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="4"
        android:layout_column="1"
        android:scaleX="2"
        android:scaleY="2"
        android:text="No Climb"
        android:layout_gravity="center"/>

    <CheckBox
        android:id="@+id/checkBox_Traversal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="4"
        android:layout_column="5"
        android:scaleX="2"
        android:scaleY="2"
        android:text="Traversal"
        android:layout_gravity="center_vertical"
        android:layout_marginLeft="40dp" />

    <CheckBox
        android:id="@+id/checkBox_HighBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="4"
        android:layout_column="4"
        android:scaleX="2"
        android:scaleY="2"
        android:text="High"
        android:layout_gravity="center" />

    <TextView
        android:id="@+id/textView_Timer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="6"
        android:layout_column="3"
        android:text="0.00"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:textSize="100dp"
        tools:layout_editor_absoluteX="970dp"
        tools:layout_editor_absoluteY="500dp" />

    <TextView
        android:id="@+id/textView_ClimbConst"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="4"
        android:layout_column="0"
        android:text="Climb Level"
        android:textSize="35dp"
        tools:layout_editor_absoluteX="970dp"
        tools:layout_editor_absoluteY="500dp" />

    <CheckBox
        android:id="@+id/checkBox_Low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleX="2"
        android:scaleY="2"
        android:text="Low"
        android:layout_row="4"
        android:layout_column="2"
        android:layout_gravity="center"

        />



    <Button
        android:id="@+id/button_StartClimb"
        android:layout_width="174dp"
        android:layout_height="74dp"
        android:layout_row="6"
        android:layout_column="2"
        android:layout_gravity="center"

        android:text="Start Climb"
        tools:layout_editor_absoluteX="742dp"
        tools:layout_editor_absoluteY="488dp" />


</GridLayout>