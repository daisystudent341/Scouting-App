<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:columnCount="6"
    android:rowCount="8">


    <TextView
        android:id="@+id/text_HighGoal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:layout_column="1"
        android:text="High Goal"
        android:textSize="35dp"
        android:layout_gravity="center_horizontal"

        />


    <Button
        android:id="@+id/button_HighPlus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="0"
        android:layout_gravity="end"
        android:text="+"
        android:textSize="90dp" />

    <TextView
        android:id="@+id/text_HighValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="1"
        android:layout_gravity="center"
        android:text="0"
        android:textSize="90dp" />

    <Button
        android:id="@+id/button_HighMinus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="2"


        android:text="-"
        android:textSize="90dp" />

    <TextView
        android:id="@+id/text_lowGoal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:layout_column="4"
        android:text="Low Goal"
        android:layout_gravity="center_horizontal"

        android:textSize="35dp" />

    <Button
        android:id="@+id/button_LowPlus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="3"
        android:layout_marginStart="50dp"
        android:text="+"
        android:textSize="90dp" />

    <TextView
        android:id="@+id/text_LowValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="4"
        android:text="0"
        android:layout_gravity="center"

        android:textSize="90dp" />


    <Button
        android:id="@+id/button_lowMinus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="5"

        android:text="-"
        android:textSize="90dp" />

    <TextView
        android:id="@+id/text_High_Missed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="1"
        android:text="High Missed"
        android:textSize="35dp" />

    <Button
        android:id="@+id/button_HighMissedMinus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="2"
        android:text="-"
        android:textSize="90dp" />

    <TextView
        android:id="@+id/text_HighMissedValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="1"
        android:layout_gravity="center"
        android:text="0"
        android:textSize="90dp" />

    <Button
        android:id="@+id/button_HighMissedPlus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="0"
        android:layout_gravity="end"
        android:text="+"
        android:textSize="90dp" />

    <Button
        android:id="@+id/button_LowMissedPlus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="3"
        android:layout_marginStart="50dp"
        android:text="+"
        android:textSize="90dp" />

    <TextView
        android:id="@+id/text_Low_Missed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="4"
        android:text="Low Missed"
        android:layout_gravity="center"

        android:textSize="35dp" />

    <TextView
        android:id="@+id/text_Low_Missed_Value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="4"
        android:layout_gravity="center"

        android:text="0"
        android:textSize="90dp" />

    <Button
        android:id="@+id/button_LowMissedMinus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="5"
        android:text="-"
        android:textSize="90dp" />


    <CheckBox
        android:id="@+id/checkBox_Taxi"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_row="5"
        android:layout_column="3"
        android:background="?android:attr/listChoiceIndicatorMultiple"
        android:button="@null" />

    <TextView
        android:id="@+id/text_Taxi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="5"
        android:layout_column="4"

        android:text="Taxi"
        android:textSize="50dp" />

    <CheckBox
        android:id="@+id/checkBox_OtherColor"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_row="4"
        android:layout_column="3"
        android:background="?android:attr/listChoiceIndicatorMultiple"
        android:button="@null" />

    <TextView
        android:id="@+id/text_Interacts"
        android:layout_width="400dp"
        android:layout_height="wrap_content"
        android:layout_row="4"
        android:layout_column="4"
        android:text="Interacts with other team's cargo"
        android:textSize="35dp" />

    <TextView
        android:id="@+id/text_Autonomous"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="4"
        android:layout_column="0"
        android:paddingStart="50dp"
        android:text="AUTONOMOUS"

        android:textSize="35dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/text_userInfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="5"
        android:layout_column="0"
        android:paddingStart="50dp"

        android:text="Info"
        android:textSize="35dp"
        android:textStyle="italic" />




    <Button
        android:id="@+id/button_quit"
        android:layout_width="182dp"
        android:layout_height="113dp"
        android:layout_row="7"
        android:layout_column="0"
        android:text="Quit"
        android:textSize="35dp" />

    <Button
        android:id="@+id/button_Submit"
        android:layout_width="182dp"
        android:layout_height="113dp"
        android:layout_row="7"
        android:layout_column="4"
        android:text="Auton"
        android:textSize="35dp"
        android:layout_gravity="end"
        />


</GridLayout>